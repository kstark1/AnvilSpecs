<?xml version="1.0" encoding="ISO-8859-1"?>

<!--
This specification introduces the concept of a TRACK.

We create a single "primary" track with a single attribute.
-->

<annotation-spec>
  <body>
    <track-spec name="words" type="primary">
      <attribute name="token" valuetype="String" />
    </track-spec>
  </body>
</annotation-spec>

